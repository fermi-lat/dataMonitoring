<?xml version="1.0" ?>

<alarms>

  <alarmList group="EventRates" name="Event rates" enabled="True">

    <alarmSet name="Time::Rate_TransientEvts" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="1.0" max="15.0"/>
	<error_limits   min="1.0" max="20.0"/>
      </alarm> 
    </alarmSet>
    <alarmSet name="Time::Rate_SourceEvts" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.2" max="3.0"/>
	<error_limits   min="0.1" max="5.0"/>
      </alarm> 
    </alarmSet>
    <alarmSet name="Time::Rate_DiffuseEvts" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.0" max="2.0"/>
	<error_limits   min="0.0" max="3.0"/>
      </alarm> 
    </alarmSet>

  </alarmList>

</alarms>
