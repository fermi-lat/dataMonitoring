<?xml version="1.0" ?>

<alarms>

<!-- TKR, as per Hiro's email on Jul 14, 2008
TkrMon_Trend_Number_layerOcc_TowerPlane: 0.08 max (error)
TkrMon_Trend_Number_LATAverage_stripOcc: 5E-5 max (error)
TkrMon_Trend_Number_fracSat_TowerPlane: 1E-4 max (just warning) 
  -->

  <alarmList group="TKRMON" name="TKR Monitoring" enabled="True">

    <alarmSet name="Time::Mean_towerEff_Tower" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.98" max="10.0"/>
	<error_limits   min="0.98" max="10.0"/>
      </alarm> 
    </alarmSet>

    <alarmSet name="Time::Mean_trigEff_Tower" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.9" max="10.0"/>
	<error_limits   min="0.9" max="10.0"/>
      </alarm> 
    </alarmSet>
   
    <alarmSet name="Time::Number_layerOcc_TowerPlane" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.0" max="0.08"/>
	<error_limits   min="0.0" max="0.08"/>
      </alarm> 
    </alarmSet>
   
    <alarmSet name="Time::Number_LATAverage_stripOcc" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.0" max="5.0e-5"/>
	<error_limits   min="0.0" max="5.0e-5"/>
      </alarm> 
    </alarmSet>

    <alarmSet name="Time::Number_fracSat_TowerPlane" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.0" max="1.0e-4"/>
	<error_limits   min="0.0" max="1"/>
      </alarm> 
    </alarmSet>

  </alarmList>  

</alarms>
  
