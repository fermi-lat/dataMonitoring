      </alarm> 
    </alarmSet>

    <alarmSet name="AcdPedRMS_PMTB_TH1" enabled="True">
      <alarm function="y_values" enabled="True">
	<warning_limits min="2.0" max="6.0"/>
	<error_limits   min="1.0" max="10.0"/>
        <parameter name="exclude" value="range(90, 97) + range(105, 129)"/>
      </alarm> 
    </alarmSet>

  </alarmList>

</alarms>
