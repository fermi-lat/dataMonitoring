<?xml version="1.0" ?>

<alarms>

  <alarmList group="Main" name="Main" enabled="True">
    
    <alarmSet name="TEM_BUG_INSTANCE" enabled="True">
      <alarm function="fraction" enabled="True">
	<warning_limits min="0.0" max="0.01"/>
	<error_limits   min="0.0" max="0.10"/>
      </alarm>
    </alarmSet>

    <alarmSet name="PHASE_ERROR" enabled="True">
      <alarm function="number" enabled="True">
	<warning_limits min="0.0" max="0.5"/>
	<error_limits   min="0.0" max="0.5"/>
      </alarm>
    </alarmSet>

    <alarmSet name="TIMETONE_INCOMPLETE" enabled="True">
      <alarm function="rate" enabled="True">
	<warning_limits min="0.0" max="0.50"/>
	<error_limits   min="0.0" max="2.00"/>
      </alarm>
    </alarmSet>

  </alarmList>
 
</alarms>
