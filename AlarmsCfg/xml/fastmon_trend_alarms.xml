<?xml version="1.0" ?>

<alarms>

<!-- Environment quantities -->
  <alarmList group="SpaceCraft" name="SpaceCraft base" enabled="True">
    <alarmSet name="Time::Mean_FastMon_SpaceCraft_Altitude" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="500" max="600"/>
	<error_limits   min="450"  max="700"/>
      </alarm> 
    </alarmSet>    
  </alarmList>


<!-- TKR
  -->

  <alarmList group="TKR" name="TKR base" enabled="True">
    <alarmSet name="Time::Mean_FastMon_TkrHits" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="100" max="300"/>
	<error_limits   min="50"  max="400"/>
      </alarm> 
    </alarmSet>
   
  </alarmList>
  
<!-- CAL
  -->
  
  <alarmList group="CAL" name="CAL base" enabled="True">

    <alarmSet name="Time::Mean_FastMon_CalTowerCount" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="2.5" max="4.0"/>
	<error_limits   min="2.0" max="5.0"/>
      </alarm> 
    </alarmSet>

    <!-- If we really want this, we'll have to optimize the algorithm, cause it's a lot of points.

    <alarmSet name="Time::Mean_FM_CalLogRHit_TowerCalLayerCalColumnFR" enabled="True">
      <alarm function="values" enabled="True">
	<warning_limits min="0.0" max="0.02"/>
	<error_limits   min="0.0" max="0.03"/>
      </alarm> 
    </alarmSet>

    -->
        
  </alarmList>  


</alarms>
  
