	    <cut><![CDATA[ (condsummary&30)>0 && (condsummary&32)==0]]></cut>	    
        </CUSTOM>
        <CUSTOM type="CalLogEndRangeHitCounter" name="CalLogEndRangeHitCounter_Range1_TH2" enabled="True" level="tower">
	    <expression>Range1</expression>
	    <title>Number of hits in each Cal log end for range 1 (LEX1)</title>
	    <xlabel>CAL column/side</xlabel>
	    <ylabel>CAL layer</ylabel>
	    <drawoptions>colz</drawoptions>	    
	    <cut><![CDATA[ (condsummary&30)>0 && (condsummary&32)==0]]></cut>	    
        </CUSTOM>
        <CUSTOM type="CalLogEndRangeHitCounter" name="CalLogEndRangeHitCounter_Range2_TH2" enabled="True" level="tower">
	    <expression>Range2</expression>
	    <title>Number of hits in each Cal log end for range 2 (HEX8)</title>
	    <xlabel>CAL column/side</xlabel>
	    <ylabel>CAL layer</ylabel>
	    <drawoptions>colz</drawoptions>	    
	    <cut><![CDATA[ (condsummary&30)>0 && (condsummary&32)==0]]></cut>	    
        </CUSTOM>
        <CUSTOM type="CalLogEndRangeHitCounter" name="CalLogEndRangeHitCounter_Range3_TH2" enabled="True" level="tower">
	    <expression>Range3</expression>
	    <title>Number of hits in each Cal log end for range 3 (HEX1)</title>
	    <xlabel>CAL column/side</xlabel>
	    <ylabel>CAL layer</ylabel>
	    <drawoptions>colz</drawoptions>	    
	    <cut><![CDATA[ (condsummary&30)>0 && (condsummary&32)==0]]></cut>	    
        </CUSTOM>
    </outputList>
    
    <outputList group="CAL" name="CAL base" enabled="True">
        <CUSTOM type="CalXHit_NHit_Counter_TowerCalLayer" name="CalXHit_NHit_Counter_TowerCalLayer_TH2" enabled="True">
	    <title>Number of hits in a calorimeter layer defined by the coordinates tower,callayer. Periodic triggers are NOT taken into account</title>
	    <xlabel>Tower number</xlabel>
	    <ylabel>Cal-Layer</ylabel>
	    <drawoptions>colz</drawoptions>
	    <!--expression>CalXHit_TowerCalLayerCalColumn</expression-->
	    <!--exclude>[0]</exclude-->
	    <cut><![CDATA[ (condsummary&30)>0 && (condsummary&32)==0]]></cut>	    
        </CUSTOM>

        <CUSTOM type="ZeroCalXHit_NHit_Counter_TowerCalLayer" name="ZeroCalXHit_NHit_Counter_TowerCalLayer_TH2" enabled="True">
	    <title>Number of times there is ZERO hits in a calorimeter layer defined by the coordinates tower,callayer. Periodic triggers are NOT taken into account</title>
	    <xlabel>Tower number</xlabel>
	    <ylabel>Cal-Layer</ylabel>
	    <drawoptions>colz</drawoptions>
	    <!--expression>CalXHit_TowerCalLayerCalColumn</expression-->
