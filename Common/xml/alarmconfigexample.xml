<?xml version="1.0" ?>

<alarms>

    <!--
    TKR group.
    -->
    <alarmList group="TKR" name="TKR base" enabled="True">
       
			 <alarmSet name="Time/nEvents" enabled="True">
         <alarm function="values" enabled="True">
            <warning_limits min="4500" max="5000"/>
            <error_limits min="4000" max="5500"/>
	 </alarm> 
       </alarmSet>   
			  
       <alarmSet name="IsocDataTree/tkr_strip_count" enabled="True">
         <alarm function="values" enabled="True">
            <warning_limits min="0" max="50"/>
            <error_limits min="0" max="200"/>
	 </alarm> 
       </alarmSet>   			    

    </alarmList>
       <alarmSet name="tkr_tower_strip_count_tower_*" enabled="True">
         <alarm function="x_average" enabled="True">
            <parameter name="min" value="1.3"/>
            <parameter name="max" value="12.0"/>
            <warning_limits min="21" max="30"/>
            <error_limits min="5.0" max="40.0"/>
	 </alarm> 
       </alarmSet>

    </alarmList>

</alarms>
